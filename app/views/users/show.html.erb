<center>
  <% if current_user %>
  <div class="panel panel-info">
    <div class="panel-heading">
      <div class="panel-body">
        <h1><%= "-#{current_user.username}'s List Central-" %><h1>
        </div>
      </div>
    </div>
        <ul>
          <%= render @lists %>
        </ul>
        <em><h3><%= link_to "Create a New List", new_list_path, class: 'new_list' %></h3></em>
    <% else %>
        <%= link_to "Please Log in", login_path %>
    <% end %>
</center>
